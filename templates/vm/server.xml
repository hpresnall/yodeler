<domain type="kvm" >
  <name></name>
  <memory unit="MiB"></memory>
  <vcpu></vcpu>
  <os>
    <type arch="x86_64" machine="q35">hvm</type>
  </os>
  <features>
    <acpi />
    <apic />
  </features>
  <cpu mode="host-passthrough" check="none" />
  <devices>
    <console type="pty">
      <target type="serial" port="0" />
    </console>
    <controller type="usb" model="none" /><!-- disable USB -->
  </devices>
</domain>
